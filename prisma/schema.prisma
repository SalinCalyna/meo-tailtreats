generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db" // ชื่อไฟล์ SQLite (dev.db)
}

model Product {
  id          Int      @id @default(autoincrement())
  name        String
  description String
  price       Float
  category    String
}

model User {
  id       Int     @id @default(autoincrement())
  name     String
  email    String  @unique
  password String
}
